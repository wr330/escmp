    </PropertyDef>
    <PropertyDef name="timeqifei">
      <ClientEvent name="onSet">var para = arg.entity.get(&quot;para&quot;);&#xD;
if(para)&#xD;
    return;&#xD;
var fdate = arg.entity.get(&quot;fdate&quot;);&#xD;
var year = fdate.getFullYear();&#xD;
var month = fdate.getMonth();&#xD;
var date = fdate.getDate();&#xD;
var time = arg.value;&#xD;
time.setYear(year);&#xD;
time.setMonth(month);&#xD;
time.setDate(date);&#xD;
arg.value = time;&#xD;
</ClientEvent>
      <Property name="dataType">Time</Property>
      <Property name="label">起飞时间</Property>
      <Property name="defaultValue">00:00:00</Property>
    </PropertyDef>
    <PropertyDef name="timeland">
      <ClientEvent name="onSet">var para = arg.entity.get(&quot;para&quot;);&#xD;
if(para)&#xD;
    return;&#xD;
var fdate = arg.entity.get(&quot;fdate&quot;);&#xD;
var year = fdate.getFullYear();&#xD;
var month = fdate.getMonth();&#xD;
var date = fdate.getDate();&#xD;
var time = arg.value;&#xD;
time.setYear(year);&#xD;
time.setMonth(month);&#xD;
time.setDate(date);&#xD;
arg.value = time;</ClientEvent>
      <Property name="dataType">Time</Property>
      <Property name="label">着陆时间</Property>
      <Property name="defaultValue">00:00:00</Property>
    </PropertyDef>
    <PropertyDef name="timefly">
      <ClientEvent name="onGet">var timeland = arg.entity.get(&quot;timeland&quot;);&#xD;
var timeqifei = arg.entity.get(&quot;timeqifei&quot;);&#xD;
if(timeland != null &amp;&amp; timeqifei != null){&#xD;
    var time = (timeland.getTime()-timeqifei.getTime())/1000;&#xD;
    var miao = time%60;&#xD;
    var fen = (parseInt(time/60))%60;&#xD;
    var shi = parseInt(time/3600);&#xD;
    arg.value = (shi&lt;10?&quot;0&quot;+shi:shi) + &quot;:&quot; +  (fen&lt;10?&quot;0&quot;+fen:fen) + &quot;:&quot; +  (miao&lt;10?&quot;0&quot;+miao:miao);&#xD;
}</ClientEvent>
