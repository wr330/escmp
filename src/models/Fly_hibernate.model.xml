      <ClientEvent name="onSet">arg.entity.set(&quot;para&quot;,true);&#xD;
var year = arg.value.getFullYear();&#xD;
var month = arg.value.getMonth();&#xD;
var date = arg.value.getDate();&#xD;
var time = arg.entity.get(&quot;timeqifei&quot;);&#xD;
time.setYear(year);&#xD;
time.setMonth(month);&#xD;
time.setDate(date);&#xD;
arg.entity.set(&quot;timeqifei&quot;,time);&#xD;
time = arg.entity.get(&quot;timeland&quot;);&#xD;
time.setYear(year);&#xD;
time.setMonth(month);&#xD;
time.setDate(date);&#xD;
arg.entity.set(&quot;timeland&quot;,time);&#xD;
arg.entity.set(&quot;para&quot;,false);&#xD;
</ClientEvent>
