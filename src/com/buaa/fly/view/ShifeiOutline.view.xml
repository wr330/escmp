    <AjaxAction id="ajaxActionCheck">
      <ClientEvent name="onSuccess">var guid = &quot;&quot;;&#xD;
for(var i=1;i&lt;=32;i++){&#xD;
	var n = Math.floor(Math.random()*16.0).toString(16);&#xD;
	guid += n;&#xD;
	if((i==8)||(i==12)||(i==16)||(i==20))&#xD;
		  guid += &quot;-&quot;;&#xD;
}&#xD;
var cur=view.id('dataSetSubject').getData(&quot;!CURRENT_ROOT&quot;);&#xD;
var result=self.get(&quot;returnValue&quot;);&#xD;
if(!result){&#xD;
	if (cur) {&#xD;
	view.id('dataSetSubject').getData(&quot;!CURRENT_ROOT.outlineexecution&quot;).insert({project:{oid:cur.get(&quot;oid&quot;),name:cur.get(&quot;name&quot;)},aircrafttype:cur.get(&quot;ftype&quot;),oid:guid});&#xD;
	view.id('diaOutlineexecution').show();&#xD;
	}&#xD;
}else&#xD;
	dorado.MessageBox.alert(result);&#xD;
</ClientEvent>
