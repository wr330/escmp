	dorado.MessageBox.confirm('您是否确认删除此大纲内容?', function(){&#xD;
	&#xD;
		cur.remove();&#xD;
		uaSubject.execute();	&#xD;
		}); &#xD;
	}&#xD;
&#xD;
else&#xD;
dorado.MessageBox.alert(&quot;请选择删除的节点!&quot;);</ClientEvent>
