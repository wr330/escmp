<?xml version="1.0" encoding="UTF-8"?>
<ViewConfig>
  <Property name="metaData">
    <Property name="securityTag">Ftypes</Property>
  </Property>
  <Arguments/>
  <Context/>
  <Model>
    <DataType name="dataTypeSubject" parent="Subject">
      <Reference name="children">
        <Property name="dataType">[SELF]</Property>
        <Property name="dataProvider">subjectPR#querySubject</Property>
        <Property name="parameter">
          <Entity>
            <Property name="parentnode">$${this.oid}</Property>
            <Property name="ftype">$${this.ftype}</Property>
          </Entity>
        </Property>
      </Reference>
      <Reference name="tasklist">
        <Property name="dataType">[Tasklist]</Property>
        <Property name="dataProvider">tasklistPR#queryTaskOutline</Property>
        <Property name="parameter">
          <Entity>
            <Property name="subject">$${this.name}</Property>
            <Property name="ftype">$${this.ftype}</Property>
          </Entity>
        </Property>
      </Reference>
    </DataType>
  </Model>
  <View>
    <Property name="contentOverflow">scroll</Property>
    <DataSet id="dataSetSubject">
      <Property name="dataProvider">subjectPR#querySubject</Property>
      <Property name="dataType">[dataTypeSubject]</Property>
      <Property name="parameter">
        <Entity>
          <Property name="parentnode"></Property>
          <Property name="ftype"></Property>
        </Entity>
      </Property>
    </DataSet>
    <DataSet id="dsFtypes">
      <Property name="dataType">[Ftypes]</Property>
      <Property name="dataProvider">ftypesPR#queryFtypes</Property>
      <Property name="pageSize">20</Property>
      <Property name="loadMode">onReady</Property>
    </DataSet>
    <UpdateAction id="uaSubject">
      <Property name="dataResolver">subjectPR#saveSubject</Property>
      <Property name="successMessage">数据已保存</Property>
      <UpdateItem>
        <Property name="dataSet">dataSetSubject</Property>
      </UpdateItem>
    </UpdateAction>
    <AjaxAction id="ajaxActionCheck">
      <ClientEvent name="onSuccess">var cur=view.id('dataSetSubject').getData(&quot;!CURRENT_ROOT&quot;);&#xD;
var result=self.get(&quot;returnValue&quot;);&#xD;
if(!result){&#xD;
	if (cur) {&#xD;
	view.id('dataSetSubject').getData(&quot;!CURRENT_ROOT.outlineexecution&quot;).insert({project:{oid:cur.get(&quot;oid&quot;),name:cur.get(&quot;name&quot;)},aircrafttype:cur.get(&quot;ftype&quot;)});&#xD;
	view.id('diaOutlineexecution').show();&#xD;
	}&#xD;
}else&#xD;
	dorado.MessageBox.alert(result);&#xD;
</ClientEvent>
      <Property name="service">subjectManager#subjectIsOld</Property>
      <Property name="parameter"></Property>
    </AjaxAction>
    <AjaxAction id="ajaxActionDownloadFile">
      <ClientEvent name="onSuccess">var cur=view.id(&quot;dsOutlineexecution&quot;).getData(&quot;#&quot;);&#xD;
//window.open('/escmp/export/Fly_OutlineExecution/'+ arg.result );</ClientEvent>
      <Property name="service">outlineexecutionManager#downloadFile</Property>
    </AjaxAction>
    <DataSetDropDown id="dataSetDropDownAircraftType">
      <ClientEvent name="onValueSelect">var dataSetSubject = this.get(&quot;#dataSetSubject&quot;);&#xD;
dataSetSubject.set(&quot;parameter&quot;, {&#xD;
	ftype: arg.selectedValue&#xD;
});&#xD;
this.get(&quot;#dataTreeSubject&quot;).set(&quot;dataSet&quot;,&quot;dataSetSubject&quot;);&#xD;
dataSetSubject.flushAsync();&#xD;
var statisticjiaci = this.get(&quot;#statisticjiaci&quot;);&#xD;
statisticjiaci.set(&quot;parameter&quot;, {&#xD;
	ftype: arg.selectedValue&#xD;
});&#xD;
statisticjiaci.flushAsync();</ClientEvent>
      <Property name="autoOpen">true</Property>
