	dorado.MessageBox.confirm('您是否确认向专业室成员进行具体作业分配?', function(){
		allJoballot = allJoballot.toArray();
		allJoballot.each(function(joballot){
			if(joballot.get(&quot;workStatus&quot;) == 0){
				joballot.set(&quot;workStatus&quot;,1);
			}	
		})
		cur.set(&quot;workStatus&quot;,2);
		view.id('uaJobstatistics').execute();
	}); 
}
else{
	dorado.MessageBox.alert(&quot;没有进行具体作业分配&quot;);
