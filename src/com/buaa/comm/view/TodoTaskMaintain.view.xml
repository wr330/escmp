window.closeProcessDialog=function(type){&#xD;
	view.get(&quot;#dialogCompleteTask&quot;).hide();&#xD;
	var dataSetTodoTask = view.get(&quot;#dataSetTodoTask&quot;);&#xD;
	var dataSetExpiredTask = view.get(&quot;#dataSetExpiredTask&quot;);&#xD;
	if(type == 1){&#xD;
		dataSetTodoTask.flushAsync();&#xD;
	}else if(type == 2){&#xD;
		dataSetExpiredTask.flushAsync();&#xD;
	}else{&#xD;
